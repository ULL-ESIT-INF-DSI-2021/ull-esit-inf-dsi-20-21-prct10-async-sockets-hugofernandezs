s a new note body.\n   */\n  public set body(newBody: string) {\n    this.body_ = newBody;\n  }\n}\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"b384d4a3a77e2830f0184dda9f00afb1a8423823","contentHash":"f36ca107081da2d7ce4667adb6eefae2f132f9b08de6655176e79cd5d2ad7f4a"},"/home/usuario/Pract10/src/models/NoteManager/NoteManager.ts":{"path":"/home/usuario/Pract10/src/models/NoteManager/NoteManager.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":29}},"2":{"start":{"line":4,"column":15},"end":{"line":4,"column":32}},"3":{"start":{"line":17,"column":8},"end":{"line":17,"column":33}},"4":{"start":{"line":18,"column":8},"end":{"line":18,"column":29}},"5":{"start":{"line":19,"column":8},"end":{"line":19,"column":25}},"6":{"start":{"line":20,"column":8},"end":{"line":30,"column":9}},"7":{"start":{"line":21,"column":26},"end":{"line":21,"column":67}},"8":{"start":{"line":22,"column":12},"end":{"line":26,"column":15}},"9":{"start":{"line":23,"column":29},"end":{"line":23,"column":79}},"10":{"start":{"line":24,"column":33},"end":{"line":24,"column":60}},"11":{"start":{"line":25,"column":16},"end":{"line":25,"column":97}},"12":{"start":{"line":29,"column":12},"end":{"line":29,"column":52}},"13":{"start":{"line":37,"column":8},"end":{"line":37,"column":26}},"14":{"start":{"line":43,"column":8},"end":{"line":43,"column":27}},"15":{"start":{"line":50,"column":8},"end":{"line":50,"column":29}},"16":{"start":{"line":51,"column":8},"end":{"line":61,"column":9}},"17":{"start":{"line":52,"column":26},"end":{"line":52,"column":67}},"18":{"start":{"line":53,"column":12},"end":{"line":57,"column":15}},"19":{"start":{"line":54,"column":29},"end":{"line":54,"column":79}},"20":{"start":{"line":55,"column":33},"end":{"line":55,"column":60}},"21":{"start":{"line":56,"column":16},"end":{"line":56,"column":97}},"22":{"start":{"line":60,"column":12},"end":{"line":60,"column":5